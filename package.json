{
	"name": "react-architecture",
	"version": "1.0.0",
	"descriptiocn": "A small project of architecture to avoid to start a project from the very beginning using React + Redux in a simple way.",
	"author": "Carlos Abdalla",
	"license": "MIT",
	"scripts": {
		"precommit": "lint-staged",
		"start": "npm-run-all --parallel start-dash",
		"begin": "babel-node tools/server.js",
		"format": "./node_modules/.bin/prettier-eslint --write src/**/*.js",
		"lint": "node_modules/.bin/esw webpack.config.* src tools",
		"lint:watch": "npm run lint -- --watch",
		"start-dash": "webpack-dashboard -- babel-node tools/server.js",
		"start-message": "babel-node tools/startMessage.js",
		"test": "mocha --reporter progress tools/testSetup.js \"src/**/*.test.js\"",
		"test:watch": "npm run test -- --watch",
		"clean-dist": "npm run remove-dist && mkdir dist",
		"remove-dist": "node_modules/.bin/rimraf ./dist",
		"build:html": "babel-node tools/buildHtml.js",
		"prebuild": "npm-run-all clean-dist test lint build:html",
		"build": "babel-node tools/build.js",
		"postbuild": "babel-node tools/distServer.js"
	},
	"lint-staged": {
		"*.{js,jsx}": [
			"node_modules/.bin/esw webpack.config.* src tools --fix",
			"git add"
		],
		"*.css": [
			"node_modules/.bin/prettier --write",
			"git add"
		]
	},
	"dependencies": {
		"axios": "^0.18.0",
		"domkit": "^0.0.1",
		"history": "^4.7.2",
		"prop-types": "15.6.2",
		"react": "^16.3.1",
		"react-dom": "^16.3.1",
		"react-redux": "^5.0.7",
		"react-router": "^4.2.0",
		"react-router-dom": "^4.2.2",
		"react-router-redux": "5.0.0-alpha.9",
		"redux": "4.0.0",
		"redux-form": "^7.3.0",
		"redux-thunk": "^2.2.0",
		"toastr": "^2.1.4"
	},
	"devDependencies": {
		"@babel/cli": "^7.0.0",
		"@babel/core": "^7.0.0",
		"@babel/node": "^7.0.0",
		"@babel/plugin-proposal-class-properties": "^7.0.0",
		"@babel/plugin-proposal-decorators": "^7.0.0",
		"@babel/plugin-proposal-do-expressions": "^7.0.0",
		"@babel/plugin-proposal-export-default-from": "^7.0.0",
		"@babel/plugin-proposal-export-namespace-from": "^7.0.0",
		"@babel/plugin-proposal-function-bind": "^7.0.0",
		"@babel/plugin-proposal-function-sent": "^7.0.0",
		"@babel/plugin-proposal-json-strings": "^7.0.0",
		"@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
		"@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
		"@babel/plugin-proposal-numeric-separator": "^7.0.0",
		"@babel/plugin-proposal-optional-chaining": "^7.0.0",
		"@babel/plugin-proposal-pipeline-operator": "^7.0.0",
		"@babel/plugin-proposal-throw-expressions": "^7.0.0",
		"@babel/plugin-syntax-dynamic-import": "^7.0.0",
		"@babel/plugin-syntax-import-meta": "^7.0.0",
		"@babel/polyfill": "^7.0.0",
		"@babel/preset-env": "^7.0.0",
		"@babel/preset-react": "^7.0.0",
		"@babel/register": "^7.0.0",
		"autoprefixer": "9.2.1",
		"babel-eslint": "10.0.1",
		"babel-loader": "^8.0.0",
		"babel-preset-react-hmre": "1.1.1",
		"babylon": "^6.18.0",
		"cheerio": "^1.0.0-rc.2",
		"colors": "1.3.2",
		"compression": "^1.7.2",
		"css-loader": "1.0.0",
		"dotenv": "6.1.0",
		"enzyme": "3.7.0",
		"enzyme-adapter-react-16": "1.6.0",
		"eslint": "5.7.0",
		"eslint-config-prettier": "3.1.0",
		"eslint-loader": "2.1.1",
		"eslint-plugin-async-await": "^0.0.0",
		"eslint-plugin-import": "^2.10.0",
		"eslint-plugin-jsx-a11y": "6.1.2",
		"eslint-plugin-prettier": "3.0.0",
		"eslint-plugin-react": "^7.7.0",
		"eslint-watch": "4.0.2",
		"expect": "23.6.0",
		"express": "^4.16.3",
		"extract-text-webpack-plugin": "4.0.0-beta.0",
		"file-loader": "^1.1.11",
		"husky": "1.1.2",
		"jsdom": "12.2.0",
		"lint-staged": "7.3.0",
		"mini-css-extract-plugin": "0.4.2",
		"mocha": "^5.0.5",
		"node-sass": "4.9.4",
		"npm-run-all": "^4.1.2",
		"open": "^0.0.5",
		"plop": "^2.0.0",
		"postcss-flexbugs-fixes": "4.1.0",
		"postcss-loader": "3.0.0",
		"prettier": "1.14.3",
		"prettier-eslint-cli": "4.7.1",
		"react-hot-loader": "4.3.11",
		"react-test-renderer": "16.5.2",
		"redux-immutable-state-invariant": "^2.1.0",
		"rimraf": "^2.6.2",
		"sass-loader": "7.1.0",
		"style-loader": "0.23.1",
		"url-loader": "1.1.2",
		"webpack": "4.21.0",
		"webpack-dashboard": "2.0.0",
		"webpack-dev-middleware": "3.4.0",
		"webpack-dev-server": "3.1.9",
		"webpack-hot-middleware": "2.24.3"
	}
}
